{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "enables the default permissions",
    "windows": ["main"],
    "permissions": [
        {
            "identifier": "http:default",
            "allow": [
                {
                    "url": ["https://**", "http://**"]
                }
            ],
            "deny": [
                {
                    "url": "https://private.tauri.app"
                }
            ]
        },
        "fs:default",
        "fs:allow-app-read-recursive",
        "fs:allow-app-write-recursive",
        "http:default",
        "core:default",
        "shell:allow-open"
    ]
}
